        <h5 id="users-create" class="endpoint-subheading">Create a user</h5>
        <pre class="endpoint-request highlight"><span class="endpoint-request-type">POST</span> /users HTTP/1.1</pre>
        <p class="endpoint-description text-muted">Create a user</p>

        <h6>Parameters</h6>

        <table class="table table-sm table-bordered">
          <thead class="thead-default">
            <tr>
              <th>Field</th>
              <th>Type</th>
              <th>Description</th>
            </tr>
          </thead>

          <tbody>
            <tr>
              <td>email <span class="tag tag-danger">required</label></td>
              <td>String</td>
              <td>The user's email (must by valid)</td>
            </tr>

            <tr>
              <td>firstname <span class="tag tag-danger">required</label></td>
              <td>String</td>
              <td>The user's first name</td>
            </tr>

            <tr>
              <td>lastname <span class="tag tag-danger">required</label></td>
              <td>String</td>
              <td>The user's last name</td>
            </tr>

            <tr>
              <td>middle_initial</td>
              <td>String</td>
              <td>The user's middle initial</td>
            </tr>

            <tr>
              <td>ssn <span class="tag tag-danger">required</label></td>
              <td>String</td>
              <td>The user's Social Security Number</td>
            </tr>

            <tr>
              <td>address_1</td>
              <td>String</td>
              <td>The user's address (first line)</td>
            </tr>

            <tr>
              <td>city</td>
              <td>String</td>
              <td>The user's city of residence</td>
            </tr>

            <tr>
              <td>state</td>
              <td>String</td>
              <td>The user's state of residence</td>
            </tr>

            <tr>
              <td>county</td>
              <td>String</td>
              <td>The user's county of residence</td>
            </tr>

            <tr>
              <td>zip</td>
              <td>String</td>
              <td>The user's zip code</td>
            </tr>

            <tr>
              <td>dob</td>
              <td>String</td>
              <td>The user's date of birth (Y-m-d)</td>
            </tr>
          </tbody>
        </table>

        <h5 class="endpoint-subheading">Example</h5>

        <ul class="nav nav-tabs">
          <li class="nav-item">
            <a class="nav-link active">Request</a>
          </li>
        </ul>

        <% code('http', css_class: 'highlight post') do %>
POST /users HTTP/1.1
Content-Type: application/vnd.api+json
Accept: application/vnd.api+json

{
  "data": {
    "type": "users",
    "attributes": {
      "email": "buster@bluth.co",
      "firstname": "Buster",
      "lastname": "Bluth",
      "ssn": "321214321"
    }
  }
}
        <% end %>

        <ul class="nav nav-tabs">
          <li class="nav-item">
            <a class="nav-link active">Response</a>
          </li>
