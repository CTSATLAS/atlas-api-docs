      <h2 id="general">atlas-api</h2>

      <p class="lead">Welcome to the ATLAS API documentation</p>

      <section class="endpoint">
        <h4 id="appointments" class="endpoint-heading">Appointments</h4>

        <pre class="endpoint-request highlight"><span class="endpoint-request-type">POST</span> /appointments HTTP/1.1</pre>
        <p class="endpoint-description text-muted">Request an appointment</p>

        <h6>Parameters</h6>

        <table class="table table-sm table-bordered">
          <thead class="thead-default">
            <tr>
              <th>Field</th>
              <th>Type</th>
              <th>Description</th>
            </tr>
          </thead>

          <tbody>
            <tr>
              <td>user_id</td>
              <td>Integer</td>
              <td>The id of the user requesting the appointment</td>
            </tr>

            <tr>
              <td>location_id</td>
              <td>Integer</td>
              <td>The id of the location that the user needs the appointment</td>
            </tr>

            <tr>
              <td>service_type_id</td>
              <td>Integer</td>
              <td>The id of the service type attached to the request</td>
            </tr>

            <tr>
              <td>in_office</td>
              <td>Boolean</td>
              <td>Will the appointment by in the office or virtual</td>
            </tr>
          </tbody>
        </table>

        <h5 class="endpoint-subheading">Example</h5>

        <ul class="nav nav-tabs">
          <li class="nav-item">
            <a class="nav-link active">Request</a>
          </li>
        </ul>

        <% code('http', css_class: 'highlight post') do %>
POST /appointments HTTP/1.1
Content-Type: application/vnd.api+json
Accept: application/vnd.api+json

{
  "data": {
    "type": "appointments",
    "attributes": {
      "user_id": 4,
      "location_id": 1,
      "service_type_id": 6,
      "in_office": 1
    }
  }
}
        <% end %>

        <ul class="nav nav-tabs">
          <li class="nav-item">
            <a class="nav-link active">Response</a>
          </li>
        </ul>

        <% code('http', css_class: 'highlight post') do %>
HTTP/1.1 200 OK
        <% end %>
      </section>

